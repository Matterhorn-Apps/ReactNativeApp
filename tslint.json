{
    "extends": [
      "tslint:recommended",
      "tslint-eslint-rules",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": false,
      "jsx-no-lambda": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "quotemark": [true, "single", "jsx-double"]
    }
  }